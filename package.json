{
  "name": "mkdocs-site",
  "private": true,
  "scripts": {
    "predev": "python -c \"import shutil; shutil.rmtree('site', ignore_errors=True)\"",
    "dev": "python -m mkdocs serve -f mkdocs.yml --dev-addr=localhost:8000 --livereload",
    "build": "python -m mkdocs build -f mkdocs.yml",
    "check": "python -m mkdocs build -f mkdocs.yml --strict"
  }
}
